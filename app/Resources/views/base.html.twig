<!DOCTYPE html>
<html>
    <head>
        <meta charset="UTF-8" />
        <title>
            {% block title %}
                EthicalMedTech Conferences
            {% endblock %}
        </title>
        {% block stylesheets %}
            <link rel="stylesheet" type="text/css" href="{{ asset('resources/css/styles.css') }}" />
            {#<link rel="stylesheet" type="text/css" href="{{ asset('resources/css/jquery-ui-styles/jquery-ui-1.10.4.custom.min.css') }}" />#}

            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/bmatznerjqueryui/css/smoothness/jquery-ui.css') }}" />
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/bmatznerjqueryui/css/smoothness/jquery.ui.theme.css') }}" />
            <link rel="stylesheet" type="text/css" media="screen" href="{{ asset('resources/js/timepicker/timepicker-ui.css') }}" />
        {% endblock %}
        
        {% block editStylesheets %}
            <link rel="stylesheet" type="text/css" href="{{ asset('bundles/bmatznerjqueryui/css/smoothness/jquery.ui.theme.css') }}" />
            <link rel="stylesheet" type="text/css" href="{{asset('resources/css/select2/select2.min.css')}}">
            <link rel="stylesheet" type="text/css" href="{{asset('resources/css/validationEngine.jquery.css')}}">
            <link rel="stylesheet" type="text/css" href="{{asset('resources/css/select-multiple.css')}}">
        {% endblock %}

        <script type="text/javascript" src="{{asset('resources/js/jquery-1.11.2.min.js')}}"></script>
        
        {% block editScripts %}
            <script type="text/javascript" src="{{ asset('bundles/bmatznerjqueryui/js/minified/jquery-ui.min.js') }}"></script>
            <script type="text/javascript" src="{{asset('resources/js/timepicker/timepicker-ui.js')}}"></script>
            <script type="text/javascript" src="{{asset('resources/js/select2/select2.min.js')}}"></script>
            <script type="text/javascript" src="{{asset('resources/js/jquery.validationEngine.js')}}"></script>
            <script type="text/javascript" src="{{asset('resources/js/jquery.validationEngine-en.js')}}"></script>
            <script type="text/javascript" src="{{asset('resources/js/jquery.select-multiple.js')}}"></script> 
        
        {% endblock %}
            <link rel="icon" type="image/x-icon" href="{{ asset('favicon.ico') }}" />
            <script type="text/javascript" src="{{asset('resources/js/tipsify.js')}}"></script>
            <script type="text/javascript" src="{{asset('resources/js/common.js')}}"></script>
        {% block tinyMCEScripts %}
            <script type="text/javascript" src="{{asset('resources/js/tiny_mce/tiny_mce.js')}}"></script>
        {% endblock %}

        <!--[if lt IE 9]>
            <script src="http://html5shiv.googlecode.com/svn/trunk/html5.js"></script>
        <![endif]-->                                  
    </head>
    <body>
         <!-- wrapper -->
        <div class="wrapper">
        {% block body %}
            <header>
                {% block header %}
                 <a target="_new" href="http://www.ethicalmedtech.eu/" class="home"><img src="{{asset("resources/images/logo.jpg")}}" alt="ethical medtech logo" /></a>
  
                {% block banner %}
                <div class="banner"><img src="{{asset("resources/images/header-image.jpg")}}" alt=""></div>
                {% endblock %}
                {% block nav %}
                <div class="navigation">
                    <div class="left-navigation"></div>
                    <div class="right-navigation"></div>
                </div>
                {% endblock %}
                {% block SignIn %}
                {% endblock %}
      
                {% endblock %}
            </header>
            
            {% block top %}{% endblock %}
   
            {% block topcontent %}{% endblock %}
            
            <section class="content">
                {% block content %}{% endblock %}
            </section>
                
            {% block bottom %}{% endblock %}
            
            <footer>
                {% include "footer.html.twig" %}   
            </footer>
        {% endblock %}
        </div>
        {% block bottom_scripts %}
            
        {% endblock %}
        
    </body>
</html>
