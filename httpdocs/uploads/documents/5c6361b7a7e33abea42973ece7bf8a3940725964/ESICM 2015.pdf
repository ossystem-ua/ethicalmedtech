	$("div.sync").click(function() {

		var action = $(this).attr("class").replace("sync", "").replace(/\s+/g, "");
		
		var form = "";

		if(action == "signin") form = signInForm();
		if(action == "signout") form = signOutForm();
		
		$("div.view").html("<div class=\"form\">" + form + "</div>");

		modal = $("div.view");
		modal.plainModal("open", { duration: 100 });

		modal.on("plainmodalopen", function(event) { 
			
			$("div.plainmodal-hint").html(
			
				$("div.plainmodal-hint").html().replace("&nbsp;", "To close this view click besides the overlay, on the X icon (upper right corner) or press ESC") 
			);

			modalState = true;
		});

		modal.on("plainmodalclose", function(event) { 
			
			modalState = false;
		});

		$(".plainmodal-close").click(function() {

			modal.plainModal("close", { duration: 100 });
		});

		if(action == "signin") {

			formAction();
			signInAction();
		}

		if(action == "signout") {

			signOutAction();			
		}
	}); 

	function formAction() {

		$("div.form div.menu li").off("click");

		$("div.form div.menu li").click(function() {

			var form = "";
			var action = $(this).attr("class").replace("active", "").replace(/\s+/g, "");

			if(action == "signin") form = signInForm();
			if(action == "signout") form = signOutForm();
			if(action == "mail") form = mailForm();
			
			$("div.view").html("<div class=\"form\">" + form + "</div>");

			formAction();

			if(action == "signin") signInAction();
			if(action == "signout") signOutAction();
			if(action == "mail") mailAction();
		});
	}

