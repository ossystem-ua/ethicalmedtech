			//console.dir(textStatus);
			//console.dir(errorThrown);

			if(textStatus != "abort") { 
				
				$("div#ajaxerror").fadeIn(200).delay(2000).fadeOut(200);
				stopLoader();
			}
		});
	}
	
	function drawItinerary(scroll) {
		
		var out = "";

		var lastBlock = lastKey(itineraryData);

		var siti = (localStorage.esicm2015ItinerarySessions != "")
			?localStorage.esicm2015ItinerarySessions.split(",")
			:[];

		var piti = (localStorage.esicm2015ItineraryPresentations != "")
			?localStorage.esicm2015ItineraryPresentations.split(",")
			:[]; 

		// Itinerary

		if($.isEmptyObject(itineraryData) == false) {

			$.each(itineraryData, function(bkey, bvalue) {

				var block = bkey.split(", ");

				var caption = "<div>" + block[0] + "<span>" + block[1] + "</span></div>";

				out += "<div id=\"" + itineraryBlocks[bkey] + "\" class=\"header\"><div class=\"title\">" + caption + "</div></div>"
					+ "<div id=\"" + itineraryBlocks[bkey] + "\" class=\"block list\">"
					+ "<table>";
			
				$.each(bvalue, function(skey, svalue) {

					// Session

					svalue.title = highlight(svalue.title);

					var title = (svalue.code != "")?svalue.code + ", " + svalue.title:svalue.title;
					var time = svalue.type + ", " + svalue.time;
					var active = ($.inArray(svalue.id, siti) >= 0)?" active":"";
					var hint = ($.inArray(svalue.id, siti) >= 0)?"Remove session from favourites":"Add session to favourites";
					var person = (parseInt(svalue.pers) > 0)?"<span id=\"id" + svalue.id +"\" class=\"person\" title=\"This session lists a person that you were searching for. Click to preview.\"></span>":"";

					out += "<thead id=\"id" + svalue.id + "\">";

					if(skey == 1) {
					
						out += "<tr>"
							+ "<td class=\"color\" style=\"background: #" + svalue.background + ";\"></td>"
							+ "<td class=\"code\">&nbsp;</td>"
							+ "<td class=\"title\">&nbsp;</td>"
							+ "<td class=\"time\">&nbsp;</td>"
							+ "<td class=\"view\">&nbsp;</td>"
							+ "<td class=\"iti\">&nbsp;</td>"
							+ "</tr>";
					}

					out += "<tr class=\"top\">"
						+ "<td class=\"color\" style=\"background: #" + svalue.background + ";\"></td>"
						+ "<td colspan=\"2\">" + time + "</span></td>"
						+ "<td class=\"time\">&nbsp;</td>"
						+ "<td class=\"view\"></td>"
						+ "<td class=\"iti\"></td>"
						+ "</tr>"

						+ "<tr>"
						+ "<td class=\"color\" style=\"background: #" + svalue.background + ";\"></td>"
						+ "<td colspan=\"2\" class=\"title\"><a href=\"#\" id=\"id" + svalue.id +"\" title=\"Preview this session\">" + title + "</a>" + person + "</td>"
						+ "<td class=\"time\">&nbsp;</td>"
						+ "<td class=\"view\"><a href=\"#\" id=\"id" + svalue.id +"\" title=\"Preview this session\"><span></span></a></td>"
						+ "<td class=\"iti\"><span class=\"id" + svalue.id + active + "\" title=\"" + hint + "\"></span></td>"
						+ "</tr>"

						+ "</thead>";

					if($.isEmptyObject(svalue.pres) == false) {

						// Presentations

						var lastPresentation = lastKey(svalue.pres);

						out += "<tbody id=\"id" + svalue.id + "\">";

						$.each(svalue.pres, function(pkey, pvalue) {

							pvalue.title = highlight(pvalue.title);

							var view = (pvalue.abs != "")
								?["<a href=\"#\" id=\"id" + svalue.id + "-" + pvalue.id + "\" title=\"Preview this presentation\">","</a>","<span></span>"]
								:["","",""];

							var view = ["<a href=\"#\" id=\"id" + svalue.id + "-" + pvalue.id + "\" title=\"Preview this presentation\">","</a>","<span></span>"];
							var active = ($.inArray(pvalue.id, piti) >= 0)?" active":"";
							var hint = ($.inArray(pvalue.id, piti) >= 0)?"Remove presentation from favourites":"Add presentation to favourites";
							var person = (parseInt(pvalue.pers) > 0)?"<span id=\"id" + svalue.id + "-" + pvalue.id + "\" class=\"person\" title=\"This presentation lists a person that you were searching for. Click to preview.\"></span>":"";
							var bottom = (pkey == lastPresentation)?" class=\"bottom\"":"";

							out += "<tr" + bottom + ">"
								+ "<td class=\"color\" style=\"background: #" + svalue.background + ";\"></td>"

							if(svalue.layout == "code-title-time") {
								
								out += "<td class=\"code\">" + view[0] + pvalue.code + view[1] + "</td>"
									+ "<td class=\"title\">" + view[0] + pvalue.title + view[1] + person + "</td>"
									+ "<td class=\"time\">" + pvalue.time + "</td>";
							
							} else if(svalue.layout == "code-title") {
								
								out += "<td class=\"code\">" + view[0] + pvalue.code + view[1] + "</td>"
									+ "<td colspan=\"2\" class=\"title\">" + view[0] + pvalue.title + view[1] + person + "</td>";
							
							} else if(svalue.layout == "title-time") {
								
								out += "<td colspan=\"2\" class=\"title\">" + view[0] + pvalue.title + view[1] + person + "</td>"
									+ "<td class=\"time\">" + pvalue.time + "</td>";
							
							} else if(svalue.layout == "title") {
								
								out += "<td colspan=\"3\" class=\"title\">" + view[0] + pvalue.title + view[1] + person + "</td>";	
							}

							out += "<td class=\"view\">" + view[0] + view[2] + view[1] + "</td>" 
								+ "<td class=\"iti\"><span class=\"id" + svalue.id + "-" + pvalue.id + active + "\" title=\"" + hint + "\"></span></td>"
								+ "</tr>";
						});

						out += "</tbody>";
					}
				});

				out += "</table></div>";
			});

		} else {

			out += "<div class=\"empty\">No data found</div>";
		}

		$("div#content").html(out);

		stopLoader();

		setScroll();
		setItinerary();
		setSessions();
		setPresentations();

		if(scroll) $(window).scrollTop(0);
	} 

	function setSessions() {

		$("div.block.list table thead a, div.block.list table thead span.person, div.session").off("click");

		$("div.block.list table thead a, div.block.list table thead span.person, div.session").click(function() {

			var id = $(this).attr("id").replace(/[a-z\s]/g, "");
			if(id.slice(-1) == ",") id = id.substr(0, (id.length - 1));
			
			drawSession(id);

			return false;
		});
	} 
	
	function drawSession(id) {

		startLoader();

		request = $.ajax({

			type: "POST",
			url: "?fetch=session",
			data: { id: id },
			
			dataType: "json",

		}).done(function(data) {

			//console.dir(data);

			var siti = (localStorage.esicm2015ItinerarySessions != "")
				?localStorage.esicm2015ItinerarySessions.split(",")
				:[];

			var piti = (localStorage.esicm2015ItineraryPresentations != "")
				?localStorage.esicm2015ItineraryPresentations.split(",")
				:[]; 

			var out = "<table class=\"sessions\">";

			$.each(data, function(skey, svalue) {
				
				// Session 

				var active = ($.inArray(svalue.id, siti) >= 0)?" active":"";

				svalue.title = highlight(svalue.title);
				
				var title = (svalue.code != "")?svalue.code + ", " + svalue.title:svalue.title;
				var hint = ($.inArray(svalue.id, siti) >= 0)?"Remove session from favourites":"Add session to favourites";

				out += "<thead style=\"color: #" + svalue.color + "; background: #" + svalue.background + ";\" id=\"id" + svalue.id + "\">"
					
					+ "<tr>"
					+ "<td class=\"code\"></td>"
					+ "<td class=\"title\"></td>"
					+ "<td class=\"time\"></td>"
					+ "<td class=\"view\"></td>"
					+ "<td class=\"iti\"></td>"
					+ "</tr>";

				if(svalue.texttop != "") {

					out += "<tr class=\"top\">"
						+ "<td colspan=\"3\">" + svalue.texttop + "</td>"
						+ "<td class=\"view\"></td>"
						+ "<td class=\"iti\"></td>"
						+ "</tr>";
				}

				var white = (svalue.color.match(/ffffff|fff|white/i))?" white":"";

				if(svalue.group != "") svalue.type = svalue.group + "<br>" + svalue.type;

				out += "<tr class=\"top\">"
					+ "<td colspan=\"3\">" + svalue.type + "</td>"
					+ "<td class=\"view\"></td>"
					+ "<td class=\"iti\"></td>"
					+ "</tr>"

					+ "<tr class=\"bottom\">"
					+ "<td colspan=\"3\" style=\"padding-right: 6px;\">" + svalue.date + ", " + svalue.time + "<span class=\"right\">" + svalue.room + "</span></td>"
					+ "<td class=\"view\"></td>"
					+ "<td class=\"iti\"></td>"
					+ "</tr>"
					
					+ "<tr class=\"bottom\">"
					+ "<td colspan=\"3\" class=\"title\">" + title + "</td>"
					+ "<td class=\"view\"></td>"
					+ "<td class=\"iti\"><span class=\"id" + svalue.id + white + active + "\" title=\"" + hint + "\"></span></td>"
					+ "</tr>";

				if(svalue.outline != "") {

					out += "<tr class=\"bottom\">"
						+ "<td colspan=\"3\">" + svalue.outline + "</td>"
						+ "<td class=\"view\"></td>"
						+ "<td class=\"iti\"></td>"
						+ "</tr>";
				}

				// Chairs 

				if(svalue.pers != undefined) {

					out += "<tr class=\"bottom\">"
						+ "<td colspan=\"3\" class=\"chairs\"><ul>"
						+ "<li><b>Chairs:</b></li>";

					$.each(svalue.pers, function(ckey, cvalue) {

						cvalue = highlight(cvalue);

						out += "<li>" + cvalue + "</li>";
					});

					out += "</ul></td>"
						+ "<td class=\"view\"></td>"
						+ "<td class=\"iti\"></td>"
						+ "</tr>";
				}

				if(svalue.textmiddle != "") {

					out += "<tr class=\"top\">"
						+ "<td colspan=\"3\">" + svalue.textmiddle + "</td>"
						+ "<td class=\"view\"></td>"
						+ "<td class=\"iti\"></td>"
						+ "</tr>";
				}

				out += "</thead>";

				// Presenations 

				if(svalue.pres != undefined) { 

					out += "<tbody id=\"id" + svalue.id + "\">";

					$.each(svalue.pres, function(pkey, pvalue) {
						
						var view = (pvalue.abs != "")
							?["<a href=\"#\" id=\"id" + id + "-" + pvalue.id + "\" title=\"Preview this presentation\">","</a>","<span></span>"]
							:["","",""];

						var view = ["<a href=\"#\" id=\"id" + id + "-" + pvalue.id + "\"title=\"Preview this presentation\">","</a>","<span></span>"];

						var active = ($.inArray(pvalue.id, piti) >= 0)?" active":"";

						var hint = ($.inArray(pvalue.id, piti) >= 0)?"Remove presentation from favourites":"Add presentation to favourites";

						pvalue.title = highlight(pvalue.title);

						out += "<tr class=\"top\">";

						if(svalue.layout == "code-title-time") {

							out += "<td class=\"code\"><div>" + view[0] + pvalue.code + view[1] + "</div></td>"
								+ "<td class=\"title\"><div>" + view[0] + pvalue.title + view[1] + "</div></td>"
								+ "<td class=\"time\">" + pvalue.time + "</td>";

						} else if(svalue.layout == "code-title") {

							out += "<td class=\"code\"><div>" + view[0] + pvalue.code + view[1] + "</div></td>"
								+ "<td colspan=\"2\" class=\"title\"><div>" + view[0] + pvalue.title + view[1] + "</div></td>";

						} else if(svalue.layout == "title-time") {

							out += "<td colspan=\"2\" class=\"title\">" + view[0] + pvalue.title + view[1]+ "</td>"
								+ "<td class=\"time\"><div>" + pvalue.time + "</div></td>";

						} else if(svalue.layout == "title") {

							out += "<td colspan=\"3\" class=\"title\"><div>" + view[0] + pvalue.title + view[1] + "</div></td>";
						}

						out += "<td class=\"view\"><div>" + view[0] + view[2] + view[1] + "</div></td>"
							+ "<td class=\"iti\"><span class=\"id" + svalue.id + "-" + pvalue.id + active + "\" title=\"" + hint + "\"></span></td>"
							+ "</tr>";

						// Speakers 

						if(pvalue.pers != undefined) {

							var speakers = "";

							$.each(pvalue.pers, function(rkey, rvalue) {

								rvalue = highlight(rvalue);

								speakers += "<li>" + rvalue + "</li>";
							});

							out += "<tr>";

							if(svalue.layout == "code-title-time") {

								out += "<td class=\"code\"></td>"
									+ "<td class=\"speakers\"><ul>" + speakers + "</ul></td>"
									+ "<td class=\"time\"></td>";

							} else if(svalue.layout == "code-title") {

								out += "<td class=\"code\"></td>"
									+ "<td colspan=\"2\" class=\"speakers\"><ul>" + speakers + "</ul></td>";

							} else if(svalue.layout == "title-time") {

								out += "<td colspan=\"2\" class=\"speakers\"><ul>" + speakers + "</ul></td>"
									+ "<td class=\"time\"></td>";

							} else if(svalue.layout == "title") {

								out += "<td colspan=\"3\" class=\"speakers\"><ul>" + speakers + "</ul></td>";
							}

							out += "<td class=\"view\"></td>"
								+ "<td class=\"iti\"></td>"
								+ "</tr>";
						};
					});
				}

				if(svalue.textbottom != "") {

					out += "<tr class=\"top\">"
						+ "<td colspan=\"3\">" + svalue.textbottom + "</td>"
						+ "<td class=\"view\"></td>"
						+ "<td class=\"iti\"></td>"
						+ "</tr>";
				}

				if(svalue.pres != undefined || svalue.textbottom != "") {

					out += "<tr class=\"bottom\">"
						+ "<td class=\"code\"></td>"
						+ "<td class=\"title\"></td>"
						+ "<td class=\"time\"></td>"
						+ "<td class=\"iti\"></td>"
						+ "</tr>";
				}

				out	+=  "</tbody>";
			});

			out += "<table>";

			$("div.view").html(out);

			modal = $("div.view");
			modal.plainModal("open", { duration: 100 });

			modal.on("plainmodalopen", function(event) { 
				
				$("div.plainmodal-hint").html(
				
					$("div.plainmodal-hint").html().replace("&nbsp;", "To close this view click besides the overlay, on the X icon (upper right corner) or press ESC")
				);

				modalState = true;
			});

			modal.on("plainmodalclose", function(event) { 
				
				modalState = false;
			});

			$(".plainmodal-close").click(function() {

				modal.plainModal("close", { duration: 100 });
			});

			stopLoader();

			setPresentations();
			setItinerary();

		}).fail(function(jqXHR, textStatus, errorThrown) {
			
			//console.dir(jqXHR);
			//console.dir(textStatus);
			//console.dir(errorThrown);
			
			if(textStatus != "abort") { 
				
				$("div#ajaxerror").fadeIn(200).delay(2000).fadeOut(200);
				stopLoader();
			}
		});
	} 	
	
	function setPresentations() {

		$("div.block.list table tbody a, div.block.list table tbody span.person, div.view tbody a").off("click");

		$("div.block.list table tbody a, div.block.list table tbody span.person, div.view tbody a").click(function() {

			var id = $(this).attr("id").replace(/[a-z\s]/g, "").split("-");

			drawPresentation(id[0], id[1], modalState);

			return false;
		});
	}
	
	function drawPresentation(sid, pid, state) {

		startLoader();

		request = $.ajax({

			type: "POST",
			url: "?fetch=presentation",
			data: { id: pid },
			
			dataType: "json",

		}).done(function(data) {

			//console.dir(data);

			var out = "";

			if(state) {

				out += "<div style=\"background: #eee; padding: 5px 16px 6px 16px;\">"
					+ "<span id=\"" + sid + "\" class=\"session\" style=\"text-decoration: underline; cursor: pointer;\">Back</span>"
					+ "</div>";
			}
			
			out += "<table class=\"presentation\">";

			if(data.code != "" && data.abs == "") out += "<tr class=\"code top\"><td>Code: " + data.code + "</td></tr>";
			if(data.abs != "") out += "<tr class=\"code top\"><td>Abstract: " + data.code + "</td></tr>";

			data.title = highlight(data.title);

			out += "<tr class=\"title top\"><td>" + data.title + "<br></td></tr>";

			if(data.aut != "") {
				
				data.aut = highlight(data.aut);
				out += "<tr class=\"aut top\"><td>" + data.aut + "</td></tr>";
			}
			
			if(data.inst != "") { 
				
				data.inst = highlight(data.inst);
				out += "<tr class=\"inst\"><td>" + data.inst + "</td></tr>";
			}
			
			if(data.text != "") { 
				
				data.text = highlight(data.text);
				out += "<tr class=\"text top\"><td>" + data.text + "</td></tr>";
			}

			out += "</table>";

			$("div.view").html(out);

			if(modalState == false) {

				modal = $("div.view");
				modal.plainModal("open", { duration: 100 });

				modal.on("plainmodalopen", function(event) { 
					
					$("div.plainmodal-hint").html(
					
						$("div.plainmodal-hint").html().replace("&nbsp;", "To close this view click besides the overlay, on the X icon (upper right corner) or press ESC")
					);

					modalState = true;
				});

				modal.on("plainmodalclose", function(event) { 
					
					modalState = false;
				});

				$(".plainmodal-close").click(function() {

					modal.plainModal("close", { duration: 100 });
				});
			
			} else {

				$("span.session").click(function() {

					var id = $(this).attr("id");
					drawSession(id);
				});
			}

			stopLoader();

			setSessions();
			
		}).fail(function(jqXHR, textStatus, errorThrown) {
			
			//console.dir(jqXHR);
			//console.dir(textStatus);
			//console.dir(errorThrown);
			
			if(textStatus != "abort") { 
				
				$("div#ajaxerror").fadeIn(200).delay(2000).fadeOut(200);
				stopLoader();
			}
		});
	} 
	
	/*
	 * Scroll
	 */

	function setScroll() {

		$("div.left:last > div:last").css("border-bottom", "1px solid #fff");

		var docHeight = $(document).height();
		var docScroll = 0;

		var y = $(window).scrollTop();
		var x = $(window).scrollLeft();
		
		$("div#label").css("top", - y + "px");
		$("div.subtitle").css("left", - x + "px");

		var d = [];

		$("div.header").each(function(i,e) { 
			
			var id = $(this).attr("id");

			d[i] = $("div#" + id + ".block").offset().top;
			$(this).toggleClass("fixed", (y + 104) >= d[i])
		});

		$(window).off("scroll");
		
		$(window).scroll(function() {

			docScroll = $(window).scrollTop() + $(window).height();

			y = $(window).scrollTop();
			x = $(window).scrollLeft();

			$("div#label").css("top", - y + "px");
			$("div.subtitle").css("left", - x + "px");

			$("div.header").each(function(i,e) { 

				$(this).toggleClass("fixed", (y + 104) >= d[i]) 
			});

			if(localStorage.esicm2015View == "grid") {
			
				localStorage.esicm2015GridTop = y;
				localStorage.esicm2015GridLeft = x;
			}

			if(localStorage.esicm2015View == "list") {

				if(docScroll == docHeight && listTotal > listExclude) { 

					fetchList(false);
				}
			}
		});
	} 

	function startLoader() {

		$("div.title > div").css({ "background" : "#0472b8 url(img/loader.gif) repeat-x" });
	}

	function stopLoader() {

		$("div.title > div").css({ "background" : "#0472b8" });
	} 

	var data = (localStorage.esicm2015FilterDays != "")?localStorage.esicm2015FilterDays.split(","):[];

	$("div#filter div.day").each(function(i,e) {
		
		if($.inArray($(this).attr("id"), data) >= 0) {

			$(this).removeClass("active");
		}
	});

	$("div#filter div.day").click(function() {

		var action = ($(this).attr("class").replace("day ", "") == "active")?"add":"remove";
		var id = $(this).attr("id");

		data = (localStorage.esicm2015FilterDays != "")?localStorage.esicm2015FilterDays.split(","):[];

		if(action == "add") { data.push(id); }
		else { data.splice(data.indexOf(id), 1); }

		localStorage.esicm2015FilterDays = data.join(",");

		$(this).toggleClass("active");

		loadContent();
	}); 

	$("div#filter div.select").each(function(i,e) {
		
		var area = $(this).attr("class").replace("select ", "");
		var data = "";

		if(area == "groups") data = (localStorage.esicm2015FilterGroups != "")?localStorage.esicm2015FilterGroups.split(","):[];
		if(area == "types") data = (localStorage.esicm2015FilterTypes != "")?localStorage.esicm2015FilterTypes.split(","):[];
		if(area == "themes") data = (localStorage.esicm2015FilterThemes != "")?localStorage.esicm2015FilterThemes.split(","):[];
		if(area == "rooms") data = (localStorage.esicm2015FilterRooms != "")?localStorage.esicm2015FilterRooms.split(","):[];

		var all = true;
		var clear = false;

		$("div#filter div.select." + area + " > div").each(function(i,e) {

			var id = $(this).attr("id");

			if(area == "types" && localStorage.esicm2015FilterTypes == "") $(this).css({ color: "#" + types[id][0], background: "#" + types[id][1] });

			if($.inArray(id, data) >= 0) {

				$(this).addClass("active");
				
				if(area == "types" && localStorage.esicm2015FilterTypes != "") $(this).css({ color: "#" + types[id][0], background: "#" + types[id][1] });

				clear = true;

			} else {

				all = false;
			}
		});

		if(all) {

			$(this).removeClass("clear");

			if(area != "types") $("div#filter div.select." + area + " > div").removeClass("active");

			if(area == "groups") localStorage.esicm2015FilterGroups = "";
			if(area == "types") localStorage.esicm2015FilterTypes = "";
			if(area == "themes") localStorage.esicm2015FilterThemes = "";
			if(area == "rooms") localStorage.esicm2015FilterRooms = "";

			loadContent();
		} 

		if(clear && !all) {

			$("div#filter div.label." + area + " div.clear").addClass("active").attr("title", "Click here to remove this filter");
			$("div#filter div.label." + area).addClass("active");

			clearFilter();
		}
	});

	$("div#filter div.select > div").click(function() {

		var area = $(this).parent().attr("class").replace("select ", "");
		var action = (($(this).attr("class").match(/active/) != null))?"remove":"add"; 
		var id = $(this).attr("id");

		var data = [];

		if(area == "groups") {

			data = (localStorage.esicm2015FilterGroups != "")?localStorage.esicm2015FilterGroups.split(","):[];

			if(action == "add") { data.push(id); }
			else { data.splice(data.indexOf(id), 1); }

			localStorage.esicm2015FilterGroups = data.join(",");
		}

		if(area == "types") {
			
			data = (localStorage.esicm2015FilterTypes != "")?localStorage.esicm2015FilterTypes.split(","):[];

			if(action == "add") { data.push(id); }
			else { data.splice(data.indexOf(id), 1); }

			localStorage.esicm2015FilterTypes = data.join(",");
		}

		if(area == "themes") {

			data = (localStorage.esicm2015FilterThemes != "")?localStorage.esicm2015FilterThemes.split(","):[];

			if(action == "add") { data.push(id); }
			else { data.splice(data.indexOf(id), 1); }

			localStorage.esicm2015FilterThemes = data.join(",");
		}

		if(area == "rooms") {

			data = (localStorage.esicm2015FilterRooms != "")?localStorage.esicm2015FilterRooms.split(","):[];

			if(action == "add") { data.push(id); }
			else { data.splice(data.indexOf(id), 1); }

			localStorage.esicm2015FilterRooms = data.join(",");
		}
		
		if(action == "add" && action != "types") $(this).addClass("active");
		if(action == "remove" && action != "types") $(this).removeClass("active");

		if(data.length == "0") { 
			
			$("div#filter div.label." + area + " div.clear").removeClass("active").removeAttr("title");
			$("div#filter div.label." + area).removeClass("active"); 

			if(area == "types") {

				$("div#filter div.select.types > div").each(function(i,e) {
				
					$(this).css({ color: "#" + types[$(this).attr("id")][0], background: "#" + types[$(this).attr("id")][1] });
				});
			}

		} else if(data.length == $("div#filter div.select." + area + " > div").length) {
			
			$("div#filter div.label." + area + " div.clear").removeClass("active").removeAttr("title");
			$("div#filter div.label." + area).removeClass("active");

			$("div#filter div.select." + area + " > div").removeClass("active");

			if(area == "types") {
			
				$("div#filter div.select.types > div").each(function(i,e) {
				
					$(this).css({ color: "#" + types[$(this).attr("id")][0], background: "#" + types[$(this).attr("id")][1] });
				});
			}

			if(area == "groups") localStorage.esicm2015FilterGroups = "";
			if(area == "types") localStorage.esicm2015FilterTypes = "";
			if(area == "themes") localStorage.esicm2015FilterThemes = "";
			if(area == "rooms") localStorage.esicm2015FilterRooms = "";

			loadContent();
			
		} else { 
			
			$("div#filter div.label." + area + " div.clear").addClass("active").attr("title", "Click here to remove this filter");
			$("div#filter div.label." + area).addClass("active");

			$("div#filter div.clear.active").off("click");
			
			if(area == "types" && action == "add") {

				$("div#filter div.select.types > div").each(function(i,e) {
				
					if($(this).attr("class").match(/active/) == null) {

						$(this).css({ color: "", background: "" });
					}
				});

				$(this).css({ color: "#" + types[id][0], background: "#" + types[id][1] });
			}

			if(area == "types" && action == "remove") {

				$(this).css({ color: "", background: "" });
			}

			clearFilter();
		}

		loadContent();
	});
	
	function clearFilter() {

		$("div#filter div.clear.active").click(function() {

			$(this).removeClass("active").removeAttr("title");
			
			var area = $(this).parent().attr("class").replace("label", "").replace("active", "").replace(/[\s]/g, "");

			$("div#filter div.label." + area).removeClass("active");
			$("div#filter div.select." + area + " > div").removeClass("active");

			if(area == "types") {

				$("div#filter div.select.types > div").each(function(i,e) {
				
					$(this).css({ color: "#" + types[$(this).attr("id")][0], background: "#" + types[$(this).attr("id")][1] });
				});
			}

			if(area == "groups") localStorage.esicm2015FilterGroups = "";
			if(area == "types") localStorage.esicm2015FilterTypes = "";
			if(area == "themes") localStorage.esicm2015FilterThemes = "";
			if(area == "rooms") localStorage.esicm2015FilterRooms = "";

			loadContent();
		});
	} 

	$("div#filter div.label div.toggle, div#filter div.label span.toggle").click(function() {

		var area = $(this).parent().attr("class").replace("label", "").replace("active", "").replace(/[\s]/g, "");
		var action = $(this).attr("class").replace("toggle", "").replace(/[\s]/g, "");

		if(action == "close") {

			$("div#filter div.label." + area + " div.toggle, div#filter div.label." + area + " span.toggle").removeClass("close").addClass("open");
			$("div#filter div.select." + area).animate({ height: "0px" }, 200);
		}

		if(action == "open") {

			$("div#filter div.label").not("." + area).find("div.toggle, span.toggle").removeClass("close").addClass("open");
			$("div#filter div.select").not("." + area).animate({ height: "0px" }, 200);

			var height = ($("div#filter div.select." + area + " div").length * $("div#filter div.select." + area + " div").outerHeight());

			$("div#filter div.label." + area + " div.toggle, div#filter div.label." + area + " span.toggle").removeClass("open").addClass("close");
			$("div#filter div.select." + area).animate({ height: height + "px" }, 200);
		}
	}); 

	setReset();

	var search = document.getElementById("searchbox");
	var lastSearch = search.value;

	var onSearch = function() {

		if(search.value != lastSearch && search.value != "Search the programme..." && search.value != "") { 

			lastSearch = search.value;
			request.abort();
			localStorage.esicm2015Search = lastSearch;

			setReset();
			loadContent();
		
		} else if(search.value != lastSearch && search.value != "Search the programme..." && search.value == "") {
			
			lastSearch = "";
			request.abort();
			localStorage.esicm2015Search = "";

			setReset();
			loadContent();
		}
	};

	var onSearchChange = function(event) {

		if (event.type === "focus") {

			document.addEventListener("selectionchange", onSearch, false);

		} else {

			document.removeEventListener("selectionchange", onSearch, false);
		}
	};

	if(search.addEventListener) {

		search.addEventListener("input", onSearch, false);
		search.addEventListener("focus", onSearchChange, false);
		search.addEventListener("blur", onSearchChange, false);
	
	} else if(search.attachEvent) {

		search.attachEvent("onpropertychange", onSearch);
	}

	function setReset() {

		if(localStorage.esicm2015Search == "") {
			
			$("div#reset").removeClass("active");
			$("div#reset span").removeAttr("title");
			$("div#search input").addClass("placeholder");

		} else {

			$("div#reset").addClass("active");
			$("div#reset span").attr("title", "Click here to remove this filter");
			$("div#search input").removeClass("placeholder");
		}

		resetSearch();
	}
	
	function resetSearch() {
		
		$("div#reset.active span").off("click");

		$("div#reset.active span").click(function() {
			
			$("div#reset").removeClass("active");
			$(this).parent().parent().find("input").addClass("placeholder");

			localStorage.esicm2015Search = ""; 

			$("div#search input[type=\"text\"]").val("");

			loadContent();
		});
	} 

	$("[placeholder]").focus(function() {
	
		var input = $(this);
	
		if(input.val() == input.attr("placeholder")) {
	
			input.val("");
			input.removeClass("placeholder");
		} 
	
	}).blur(function() {
	
		var input = $(this);
	
		if (input.val() == "" || input.val() == input.attr("placeholder")) {
	
			input.addClass("placeholder");
			input.val(input.attr("placeholder"));
		}
	
	}).blur().parents("form").submit(function() {
	
		$(this).find("[placeholder]").each(function(i,e) {
	
			var input = $(this);
	
			if (input.val() == input.attr("placeholder")) {
	
				input.val("");
			}
		});

	}); 
	
	function setItinerary() { 

		$("td.iti span").off("click"); 

		$("td.iti span").click(function() { 

			var loadItinerary = false;

			// Get data 

			var id = $(this).attr("class").replace(/[a-z\s]/g, "").split("-"); 

			var siti = (localStorage.esicm2015ItinerarySessions != "") 
				?localStorage.esicm2015ItinerarySessions.split(",") 
				:[]; 

			var piti = (localStorage.esicm2015ItineraryPresentations != "") 
				?localStorage.esicm2015ItineraryPresentations.split(",") 
				:[]; 

			siti = $.unique(siti);
			piti = $.unique(piti);

			var addsids = [];
			var delsids = [];

			var addpids = [];
			var delpids = [];

			// Presentation

			if(id.length == 2) { 

				var sid = id[0];
				var pid = id[1];
				
				var state = $.inArray(pid, piti); 

				if(state < 0) {

					var ptotal = ($("tbody#id" + id[0] + " tr td.iti span").length / $("tbody#id" + id[0]).length);
					var pactive = ($("tbody#id" + id[0] + " tr td.iti span.active").length / $("tbody#id" + id[0]).length);

					$("tbody td.iti span.id" + id[0] + "-" + id[1]).addClass("active").attr("title", "Remove presentation from favourites");
 
					if((pactive + 1) == ptotal) {

						$("thead td.iti span.id" + id[0]).addClass("active").attr("title", "Remove session from favourites");
						siti.push(sid);

						addsids.push(sid); 
					}
					
					piti.push(pid); 

					addpids.push(pid);
				
				} else {

					$("thead td.iti span.id" + id[0]).removeClass("active").attr("title", "Add session to favourites");
					$("tbody td.iti span.id" + id[0] + "-" + id[1]).removeClass("active").attr("title", "Add presentation to favourites");

					siti.splice(siti.indexOf(sid), 1); 
					piti.splice(piti.indexOf(pid), 1); 

					delsids.push(sid);
					delpids.push(pid);

					loadItinerary = true;
				}
			}

			// Session				

			else {

				var sid = id[0];

				var state = $.inArray(sid, siti);

				if(state < 0) {

					$("thead#id" + id[0] + " td.iti span").addClass("active").attr("title", "Remove session from favourites");

					$("tbody#id" + id[0] + " td.iti span").each(function(i,e) {

						$(this).addClass("active").attr("title", "Remove presentation from favourites");

						var pid = $(this).attr("class").replace(/[a-z\s]/g, "").split("-");
						piti.push(pid[1]);

						addpids.push(pid[1]);
					});

					siti.push(sid);

					addsids.push(sid);
				
				} else {

					$("thead#id" + id[0] + " td.iti span, tbody#id" + id[0] + " td.iti span").removeClass("active").attr("title", "Add session to favourites");

					$("tbody#id" + id[0] + " td.iti span").each(function(i,e) {

						$(this).removeClass("active").attr("title", "Add presentation to favourites");

						var pid = $(this).attr("class").replace(/[a-z\s]/g, "").split("-");
						piti.splice(piti.indexOf(pid[1]), 1); 

						delpids.push(pid[1]);
					});
 
					siti.splice(siti.indexOf(sid), 1);

					delsids.push(sid);

					loadItinerary = true;
				}							
			}

			// Set data 

			siti = $.unique(siti);
			piti = $.unique(piti);

			localStorage.esicm2015ItinerarySessions = siti.join(",");
			localStorage.esicm2015ItineraryPresentations = piti.join(",");

			var key = "";

			if($.cookie("esicm2015") != undefined) {

				startLoader();

				key = $.cookie("esicm2015");
			}

			request = $.ajax({

				type: "POST",
				url: "?validate=itinerary",
				data: { 
					
					sids: localStorage.esicm2015ItinerarySessions,
					
					addsids: addsids,
					delsids: delsids,

					pids: localStorage.esicm2015ItineraryPresentations,
					
					addpids: addpids,
					delpids: delpids,

					key: key
				},
				
				dataType: "json",

			}).done(function(data) {

				//console.dir(data);

				// Add server favorites 

				for (var i in data.sids) {

					if($.inArray(data.sids[i], siti) == -1) {

						$("thead#id" + data.sids[i] + " td.iti span").addClass("active");
					}
				}

				for (var i in data.pids) {

					if($.inArray(data.pids[i], piti) == -1) {

						$("tbody td.iti span[class*=\"-" + data.pids[i] + "\"]").addClass("active"); 
					}
				}

				// Remove invalid favorites 

				for (var i in siti) {

					if($.inArray(siti[i], data.sids) == -1) {

						$("thead#id" + siti[i] + " td.iti span").removeClass("active");
					}
				}

				for (var i in piti) {

					if($.inArray(piti[i], data.pids) == -1) {

						$("tbody td.iti span[class*=\"-" + piti[i] + "\"]").removeClass("active"); 
					}
				}

				// Set local storage

				localStorage.esicm2015ItinerarySessions = data.sids.join(",");
				localStorage.esicm2015ItineraryPresentations = data.pids.join(",");

				var link = "http://www.professionalabstracts.com/esicm2015/iplanner/?export=";
				var items = "sess=" + data.sids.join(",") + "&" + "lpr=" + data.pids.join(",");

				$("div.nav.itinerary li#pdf a").attr("href", link + "pdf&" + items);
				$("div.nav.itinerary li#word a").attr("href", link + "word&" + items);
				$("div.nav.itinerary li#calendar a").attr("href", link + "calendar&" + items);

				stopLoader();

			}).fail(function(jqXHR, textStatus, errorThrown) {
					
				//console.dir(jqXHR);
				//console.dir(textStatus);
				//console.dir(errorThrown);
				
				if(textStatus != "abort") { 
					
					$("div#ajaxerror").fadeIn(200).delay(2000).fadeOut(200);
					stopLoader();
				}
			});

			if(localStorage.esicm2015View == "itinerary" && loadItinerary) {

				loadContent();
			}
		});
	} 

