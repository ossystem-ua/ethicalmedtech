	function signInAction() {
	
		$(window).off("keyup");

		$(window).keyup(function(e) {

			if(e.which == 13) {

				siginInFormAction();

				return false;
			}
		});

		$("div.form span.button").off("click");

		$("div.form span.button").click(function() {

			siginInFormAction();
		});

		function siginInFormAction() {

			request = $.ajax({

				type: "POST",
				url: "?sync=signin",
				data: { 
					mail: $("div.form input[name=mail]").val(), 
					pass: $("div.form input[name=pass]").val(),  
				},
				
				dataType: "json",

			}).done(function(data) {

				//console.dir(data);
				
				var text = data.msg.join("<br>");
				var style = (!data.status)?"hint":"success";

				showInfo(text, style);

				if(data.key != "") {

					if($("div.form input[type=checkbox]").prop("checked")) {
						
						$.cookie("esicm2015", data.key, { expires: 365 });
						
					} else {

						$.cookie("esicm2015", data.key);
					}
					
					$("div#panel div.sync").removeClass("signin");
					$("div#panel div.sync").addClass("signout").attr("title", "Sign out");
					$("div#panel div.sync").html("Sign out<span></span>");

					setTimeout(function() {

						modal.plainModal("close", { duration: 100 });

					}, 2000);

					syncItinerary(data.key);
				}

			}).fail(function(jqXHR, textStatus, errorThrown) {

				//console.dir(jqXHR);
				//console.dir(textStatus);
				//console.dir(errorThrown);

				if(textStatus != "abort") { 
					
					$("div#ajaxerror").fadeIn(200).delay(2000).fadeOut(200);
					stopLoader();
				}
			});
		}
	}
	
	function signOutAction() {

		$(window).off("keyup");

		$(window).keyup(function(e) {

			if(e.which == 13) {

				signOutFormAction();

				return false;
			}
		});

		$("div.form span.button").off("click");

		$("div.form span.button").click(function() {

			signOutFormAction();
		}); 

		function signOutFormAction() {

			$.removeCookie("esicm2015");
			
			$("div#panel div.sync").removeClass("signout");
			$("div#panel div.sync").addClass("signin").attr("title", "Sign in to save your favourites in the cloud and synchronise them with other devices");
			$("div#panel div.sync").html("Sign in<span></span>");

			showInfo("You signed out successfully.", "success");
			
			setTimeout(function() {

				modal.plainModal("close", { duration: 100 });

			}, 2000);
		}
	}

	function mailAction() {

		$(window).off("keyup");

		$(window).keyup(function(e) {

			if(e.which == 13) {

				var action = $("div.form span.button").attr("class").replace("button", "").replace(/\s+/g, "");

				mailFormAction(action);

				return false;
			}
		});

		$("div.form span.button").off("click");

		$("div.form span.button").click(function() {

			var action = $(this).attr("class").replace("button", "").replace(/\s+/g, "");

			mailFormAction(action);
		}); 

		function mailFormAction(action) {

			request = $.ajax({

				type: "POST",
				url: "?sync=" + action,
				data: { 
					mail: $("div.form input[name=mail]").val(), 
				},
				
				dataType: "json",

			}).done(function(data) {

				//console.dir(data);
				
				var text = data.msg.join("<br>");
				var style = (!data.status)?"hint":"success";

				showInfo(text, style);

			}).fail(function(jqXHR, textStatus, errorThrown) {
			
				//console.dir(jqXHR);
				//console.dir(textStatus);
				//console.dir(errorThrown);

				if(textStatus != "abort") { 
					
					$("div#ajaxerror").fadeIn(200).delay(2000).fadeOut(200);
					stopLoader();
				}
			});
		}
	} 
	
	function showInfo(text, style) {

		$("div.form div.info").attr("class", "info " + style).html(text).show();
	} 

	$("span.help").click(function() {
		
		//console.log("help");

		$("div.view").html("<div class=\"help left\"><p class=\"help\"><b>Instructions</b></p><p><b>Display Format</b></p><p>Choose how to view the programme: in a graphical grid format or in a list format.</p><ul class=\"view\"><li class=\"grid\">Show sessions in a graphical grid view. You may adjust the width and length of the session boxes by clicking on the appropriate measures (XS, S, M, L, XL). You may preview a session by clicking on it.<span></span></li></ul><ul class=\"view\"><li class=\"list\">Show sessions in a list view. You may sort the sessions by date or by session type. Click the favourites icon to add the session to your favourites. Click on the session title to preview the session.<span></span></li><li class=\"itinerary\">Show the content of the favourites you selected in a list format. You may choose different export formats: Adobe PDF, MS Word, or the iCalender export for MS Outlook or other calendar tools.<span></span></li></ul><p><b>Favourites</b></p><ul class=\"favorites\"><li class=\"favorite\">You may add sessions and presentations to your favourites by clicking the icon behind the session or presentation title, in the grid view or in the list view.The selected items are stored in your web browsers data. The data remains accessible for this browser (on this PC), as long as you do not delete your browser history.<span></span></li></ul><p>You can save your favourites in the cloud by creating an iPLANNER account. This account allows you to use the favourites on other computers and browsers as well. Click on SIGN IN (above the search field), to create an iPLANNER account to sync your favourites.</p></div><div class=\"help right\"><p class=\"help\">&nbsp;</p><p><b>Search the programme</b></p><p>You may enter a search term into the search field in the upper right area of the page. The search phrase evaluates session and presentation titles, codes, abstract numbers, names and institutions. The search result shows immediately:</p><ul><li>In the grid view: sessions with <b>no matches</b> are greyed out (but can still be opened), sessions <b>with matches</b> are shown in the colour of their session types<li>In the list view: only sessions with search matches are listed <li>The person icon indicates that a session or presentation contains a person that matches your search term. Click on this icon to open a preview. </ul><p>Click the <span class=\"cross\"></span> icon above the search phrase box to remove the search filter.</p><p><b>Filters</b></p><p>Show selected content of the scientific programme by applying filters. You may choose specific days, session types and session rooms for your display format. Click the <span class=\"cross\"></span> icon above the respective filter to remove it.</p></div>");

		modal = $("div.view");
		modal.plainModal("open", { duration: 100 });

		modal.on("plainmodalopen", function(event) { 
			
			$("div.plainmodal-hint").html(
			
				$("div.plainmodal-hint").html().replace("&nbsp;", "To close this view click besides the overlay, on the X icon (upper right corner) or press ESC")
			);

			modalState = true;
		});

		modal.on("plainmodalclose", function(event) { 
			
			modalState = false;
		});

		$(".plainmodal-close").click(function() {

			modal.plainModal("close", { duration: 100 });
		});

	}); 
	
	function highlight(text) {

		if(localStorage.esicm2015Search != "") {

			var search = localStorage.esicm2015Search
				.replace(/[\-\[\]\/\{\}\(\)\*\+\?\.\\^\$\|]/g, "\\$&")
				.replace(/a/i, "[a]")
				.replace(/c/i, "[c]")
				.replace(/e/i, "[e]")
				.replace(/i/i, "[i]")
				.replace(/n/i, "[n]")
				.replace(/o/i, "[o]")
				.replace(/s/i, "[s]")
				.replace(/u/i, "[u]")
				.replace(/y/i, "[y]")
				.replace(/z/i, "[z]");

			var regex = new RegExp("(" + search + ")", "ig"); 
			text = text.replace(regex, "<span class=\"highlight\">$1</span>"); 
		} 

		return text;
	} 

	function lastKey(obj) {

		var out = "";

		$.each(obj, function(key, value) {
		
			out = key;
		});

		return out;
	} 

}); </script>

