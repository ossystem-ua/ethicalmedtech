	$("div.nav.grid span.tab, div.nav.list span.tab, div.nav.itinerary span.tab").click(function() {

		var mode = $(this).parent().attr("class").replace("nav ", "").replace(" active", "").replace(/\s/g, "");

		animateTabs(mode);
		
		if(mode != localStorage.esicm2015View) {

			localStorage.esicm2015View = mode;
		}
	});

	function setTabs(to) {

		$("div.nav.grid")
			.css({"right": nav.grid[to] + "px"})
			.toggleClass("active", to == "grid");

		$("div.nav.list")
			.css({"right": nav.list[to] + "px"})
			.toggleClass("active", to == "list");

		$("div.nav.itinerary")
			.css({"right": nav.itinerary[to] + "px"})
			.toggleClass("active", to == "itinerary");

		loadContent();
	}

	function animateTabs(to) {

		$("div.nav.grid")
			.animate(
				{"right": nav.grid[to] + "px"}, 
				{ duration: 100, complete: function() { if(to == "grid") loadContent(); } 
			})
			.toggleClass("active", to == "grid");

		$("div.nav.list")
			.animate(
				{"right": nav.list[to] + "px"},
				{ duration: 100, complete: function() { if(to == "list") loadContent(); } 
			})
			.toggleClass("active", to == "list");

		$("div.nav.itinerary")
			.animate(
				{"right": nav.itinerary[to] + "px"},
				{ duration: 100, complete: function() { if(to == "itinerary") loadContent(); } 
			})
			.toggleClass("active", to == "itinerary");
	}

	// Modes

	setModes();

	function setModes() {

		$("div.nav.grid ul.width li#" + localStorage.esicm2015WidthMode).addClass("active");
		$("div.nav.grid ul.height li#" + localStorage.esicm2015HeightMode).addClass("active");
		$("div.nav.list ul li#" + localStorage.esicm2015ListMode).addClass("active");
	}

	$("div.nav.grid ul.width li").click(function() {

		$("div.nav.grid ul.width li").removeClass("active");
		$(this).addClass("active");

		localStorage.esicm2015View = "grid";
