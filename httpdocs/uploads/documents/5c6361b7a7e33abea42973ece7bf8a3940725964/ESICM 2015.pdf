		if(!gridData) { loadContent(); } else { drawGrid(); }
	});
	
	$("div.nav.grid ul.height li").click(function() {

		$("div.nav.grid ul.height li").removeClass("active");
		$(this).addClass("active");

		localStorage.esicm2015View = "grid";
		localStorage.esicm2015HeightMode = $(this).attr("id");

		if(!gridData) { loadContent(); } else { drawGrid(); }
	});

	$("div.nav.list li").click(function() {

		$("div.nav.list li").removeClass("active"); 
		$(this).addClass("active");

		listExclude = 0;
		listBlocks = [];

		localStorage.esicm2015View = "list";
		localStorage.esicm2015ListMode = $(this).attr("id");

		loadContent();
	});

	// Load Content

	function loadContent() {
		
		if(localStorage.esicm2015View == "grid") {

			fetchGrid();
		}

		if(localStorage.esicm2015View == "list") { 

			listTotal = 0;
			listExclude = 0;

			listBlocks = "";
			listBlocksUsed = [];
			listData = "";

			deleteContent = true;
			fetchList(true); 
		}

		if(localStorage.esicm2015View == "itinerary") {

			fetchItinerary(true);
		}
	} 

	function fetchGrid() {
		
		localStorage.esicm2015View = "grid";
		startLoader();

		request = $.ajax({

			type: "POST",
			url: "?fetch=grid",
			data: {
				
				search: localStorage.esicm2015Search,  
				filter: {
				
					"days": localStorage.esicm2015FilterDays,
					"itinerarysessions": localStorage.esicm2015ItinerarySessions,
					"itinerarypresentations": localStorage.esicm2015ItineraryPresentations,
					"groups": localStorage.esicm2015FilterGroups, 
					"types": localStorage.esicm2015FilterTypes, 
					"themes": localStorage.esicm2015FilterThemes, 
					"rooms": localStorage.esicm2015FilterRooms 
				}
			},
			
			dataType: "json",

		}).done(function(data) {

			//console.dir(data);

			gridData = data;
			drawGrid();
		
		}).fail(function(jqXHR, textStatus, errorThrown) {

			//console.dir(jqXHR);
			//console.dir(textStatus);
			//console.dir(errorThrown);
			
			if(textStatus != "abort") { 
				
				$("div#ajaxerror").fadeIn(200).delay(2000).fadeOut(200);
				stopLoader();
			}
		});
	}
	
	function drawGrid() {
		
		var lastDay = lastKey(gridData.days);

		var grid = "";
		var label = "<div id=\"label\">";
		var passives = "";
		var groups = "";
		var sessions = "";
		
		var top = 104;
		var left = 57;

		var rooms = $.map(gridData.rooms, function(n, i) { return i; }).length;

		if($.isEmptyObject(gridData.days) == false) {

			$.each(gridData.days, function(dkey, dvalue) {

				top += 41;

				var start = parseInt(dvalue.start);
				var end = parseInt(dvalue.end);

				// Grid

				grid += "<div id=\"" + dvalue.num + "\" class=\"block grid\" style=\"width: " + ((rooms * widthModes[localStorage.esicm2015WidthMode]) + 244) + "px;\">";

				for(var i = start; i < end; i++) {

					var close = (dkey == lastDay && i == (end - 1))?" border-bottom: 1px solid #fff;":"";

					for(var j = 0; j < rooms; j++) {

						close += (j == (rooms -1))?" border-right: 1px solid #fff;":"";

						grid += "<div style=\"width: " 
							+ (widthModes[localStorage.esicm2015WidthMode] - 1) + "px; height: " 
							+ (Math.round(60 * heightModes[localStorage.esicm2015HeightMode]) - 1) + "px;" 
							+ close + "\"></div>";
					}

					grid += "<br>";
				}

				grid += "</div></div>";

				// Label

				var day = dvalue.date.split(", ");

				label += "<div id=\"" + dvalue.num + "\" class=\"header\">"
					+ "<div class=\"title\"><div>" + day[0] + "<span>" + day[1] + "</span></div></div>"
					+ "<div class=\"subtitle\" style=\"width: " + (rooms * widthModes[localStorage.esicm2015WidthMode]) + "px;\">";

				$.each(gridData.rooms, function(rkey, rvalue) {

					//label += "<div style=\"width: " + (widthModes[localStorage.esicm2015WidthMode] - 13) + "px;\"><b>" + rvalue.name + "</b><br>" + rvalue.cap + "</div>";
					label += "<div style=\"width: " + (widthModes[localStorage.esicm2015WidthMode] - 13) + "px;\"><b>" + rvalue.name + "</b></div>";
				});

				label += "</div><div class=\"edge\"></div></div><div class=\"left\">";

				for(var i = parseInt(dvalue.start); i < parseInt(dvalue.end); i++) {
					
					var hour = (i < 10)?"0" + i:i;

					label += "<div style=\"height: " 
						+ (Math.round(60 * (heightModes[localStorage.esicm2015HeightMode])) - 6) + "px;\">" 
						+ hour + ":00</div>";
				}

				label += "</div>";

				// Passives 

				if(gridData.passives[dkey] != undefined) {
					
					$.each(gridData.passives[dkey], function(pkey, pvalue) {
		
						var start = Math.round((pvalue.start - (gridData.days[dkey].start * 60)) * (heightModes[localStorage.esicm2015HeightMode]));
						var height = Math.round((pvalue.length) * (heightModes[localStorage.esicm2015HeightMode]));

						var color = "a4a4a4";
						var background = "e4e4e4";

						var title = "<b>" + pvalue.time + "</b><br>" + pvalue.title;

						var elements = 0;

						passives += "<div class=\"passives\" style=\"top: " 
							+ (top + start) + "px; left: " 
							+ left + "px; width: "
							+ ((rooms * widthModes[localStorage.esicm2015WidthMode])) + "px; height: " 
							+ height + "px; background: #" 
							+ background + "; color: #"
							+ color + "\">";

						for(var i = 0; i < (rooms / 4); i++) {

							elements = (((i + 1) * 4) > rooms)?(rooms % (i * 4)):4;

							passives += "<div style=\"width: " 
								+ ((widthModes[localStorage.esicm2015WidthMode] * elements) - 13) + "px; height: " 
								+ Math.round((pvalue.length) * (heightModes[localStorage.esicm2015HeightMode]) - 6) + "px;\">" + title + "</div>";
						}

						passives += "</div>";					
					});
				}

				// Groups 

				if(gridData.groups[dkey] != undefined) {
					
					$.each(gridData.groups[dkey], function(groom, sections) {
						
						$.each(sections, function(gstart, gvalue) {

							var start = Math.round(((gstart / 60) - (gridData.days[dkey].start * 60)) * (heightModes[localStorage.esicm2015HeightMode]));
							var room = gridData.rooms[groom].order;
							var height = (Math.round((gvalue.length) * (heightModes[localStorage.esicm2015HeightMode])) - 6);

							var color = gvalue.color;
							var background = gvalue.background;

							if(gridData.match != undefined) {

								color = "a4a4a4";
								background = "e4e4e4";

								var id = gvalue.id.split(",");

								for (var i = 0; i < id.length; i++) {

									if($.inArray(id[i], gridData.match) >= 0) {

										color = gvalue.color;
										background = gvalue.background;
									}
								}
							}

							var type = "<b>" + gvalue.time + "</b><br>" + gvalue.type;

							groups += "<div id=\"id" + gvalue.id + "\" class=\"session\" style=\"top: " 
								+ (top + start) + "px; left: " 
								+ (left + (room * widthModes[localStorage.esicm2015WidthMode])) + "px; width: "
								+ (widthModes[localStorage.esicm2015WidthMode] - 13) + "px; height: " 
								+ height + "px; background: #" 
								+ background + "; color: #"
								+ color + "\">" 
								+ type + "</div>";
						});
					});
				}

				// Sessions

				if(gridData.sessions[dkey] != undefined) {

					$.each(gridData.sessions[dkey], function(skey, svalue) {

						var start = Math.round(svalue.start - (gridData.days[dkey].start * 60)) * (heightModes[localStorage.esicm2015HeightMode]);
						var room = gridData.rooms[svalue.room].order;
						var height = (Math.round((svalue.length) * (heightModes[localStorage.esicm2015HeightMode])) - 6);

						var color = svalue.color;
						var background = svalue.background;

						if(gridData.match != undefined) {

							color = "a4a4a4";
							background = "e4e4e4";

							if($.inArray(svalue.id, gridData.match) >= 0) {

								color = svalue.color;
								background = svalue.background;
							}
						}

						var title = "<b class=\"time\">" + svalue.time + "</b><br>" + svalue.title;

						sessions += "<div id=\"id" + svalue.id +"\" class=\"session\" style=\"top: " 
							+ (top + start) + "px; left: " 
							+ (left + (room * widthModes[localStorage.esicm2015WidthMode])) + "px; width: "
							+ (widthModes[localStorage.esicm2015WidthMode] - 13) + "px; height: " 
							+ height + "px; background: #" 
							+ background + "; color: #"
							+ color + "\">" 
							+ title + "</div>";
					});
				}

				top += Math.round((end - start) * (60 * heightModes[localStorage.esicm2015HeightMode]));
			});

			label += "</div>";

			$("div#content").html(grid + passives + groups + sessions + label);

		} else {

			grid += "<div class=\"empty\">No data found</div>";

			$("div#content").html(grid);
		}

		stopLoader();

		setScroll();
		setItinerary();
		setSessions();
		setPresentations();

		$(window).scrollTop(localStorage.esicm2015GridTop);
		$(window).scrollLeft(localStorage.esicm2015GridLeft);
	} 

	function fetchList(scroll) {

		localStorage.esicm2015View = "list";
		startLoader();

		request = $.ajax({

			type: "POST",
			url: "?fetch=list",
			data: { 
				
				search: localStorage.esicm2015Search, 
				order: localStorage.esicm2015ListMode, 
				exclude: listExclude,

				filter: {
				
					"days": localStorage.esicm2015FilterDays, 
					"groups": localStorage.esicm2015FilterGroups, 
					"types": localStorage.esicm2015FilterTypes, 
					"themes": localStorage.esicm2015FilterThemes, 
					"rooms": localStorage.esicm2015FilterRooms 
				}
			},
			
			dataType: "json",

		}).done(function(data) {

			//console.dir(data);

			listTotal = data.total;
			listExclude += data.rows;

			listBlocks = data.blocks; 
			listData = data.list;

			drawList(scroll);

		}).fail(function(jqXHR, textStatus, errorThrown) {
			
			//console.dir(jqXHR);
			//console.dir(textStatus);
			//console.dir(errorThrown);

			if(textStatus != "abort") { 
				
				$("div#ajaxerror").fadeIn(200).delay(2000).fadeOut(200);
				stopLoader();
			}
		});
	}
	
	function drawList(scroll) {
		
		var out = "";

		var lastBlock = lastKey(listData);

		var siti = (localStorage.esicm2015ItinerarySessions != "")
			?localStorage.esicm2015ItinerarySessions.split(",")
			:[];

		var piti = (localStorage.esicm2015ItineraryPresentations != "")
			?localStorage.esicm2015ItineraryPresentations.split(",")
			:[]; 

		// List

		if($.isEmptyObject(listData) == false) {

			$.each(listData, function(bkey, bvalue) {

				if($.inArray(listBlocks[bkey], listBlocksUsed) == -1) {

					$("div#" + listBlocks[bkey] + ".block.list table").append(out);
					out = "";

					var block = bkey.split(", ");

					var caption = (localStorage.esicm2015ListMode == "date")
						?"<div>" + block[0] + "<span>" + block[1] + "</span></div>"
						:"<div>" + block[0] + "</div>";

					out += "<div id=\"" + listBlocks[bkey] + "\" class=\"header\"><div class=\"title\">" + caption + "</div></div>"
						+ "<div id=\"" + listBlocks[bkey] + "\" class=\"block list\">"
						+ "<table>";
				} 
			
				$.each(bvalue, function(skey, svalue) {

					// Session

					svalue.title = highlight(svalue.title);

					var title = (svalue.code != "")?svalue.code + ", " + svalue.title:svalue.title;
					
					var time = (localStorage.esicm2015ListMode == "date")
						?svalue.type + ", " + svalue.time
						:svalue.date + ", " + svalue.time;

					var active = ($.inArray(svalue.id, siti) >= 0)?" active":"";
					var hint = ($.inArray(svalue.id, siti) >= 0)?"Remove session from favourites":"Add session to favourites";
					var person = (parseInt(svalue.pers) > 0)?"<span id=\"id" + svalue.id +"\" class=\"person\" title=\"This session lists a person that you were searching for. Click to preview.\"></span>":"";

					out += "<thead id=\"id" + svalue.id + "\">";

					if(skey == 1 && $.inArray(listBlocks[bkey], listBlocksUsed) == -1) {
					
						out += "<tr>"
							+ "<td class=\"color\" style=\"background: #" + svalue.background + ";\"></td>"
							+ "<td class=\"code\">&nbsp;</td>"
							+ "<td class=\"title\">&nbsp;</td>"
							+ "<td class=\"time\">&nbsp;</td>"
							+ "<td class=\"view\">&nbsp;</td>"
							+ "<td class=\"iti\">&nbsp;</td>"
							+ "</tr>";
					}

					out += "<tr class=\"top\">"
						+ "<td class=\"color\" style=\"background: #" + svalue.background + ";\"></td>"
						+ "<td colspan=\"2\">" + time + "</span></td>"
						+ "<td class=\"time\">&nbsp;</td>"
						+ "<td class=\"view\"></td>"
						+ "<td class=\"iti\"></td>"
						+ "</tr>"

						+ "<tr>"
						+ "<td class=\"color\" style=\"background: #" + svalue.background + ";\"></td>"
						+ "<td colspan=\"2\" class=\"title\"><a href=\"#\" id=\"id" + svalue.id +"\" title=\"Preview this session\">" + title + "</a>" + person + "</td>"
						+ "<td class=\"time\">&nbsp;</td>"
						+ "<td class=\"view\"><a href=\"#\" id=\"id" + svalue.id +"\" title=\"Preview this session\"><span></span></a></td>"
						+ "<td class=\"iti\"><span class=\"id" + svalue.id + active + "\" title=\"" + hint + "\"></span></td>"
						+ "</tr>"

						+ "</thead>";

					if($.isEmptyObject(svalue.pres) == false) {

						// Presentations

						var lastPresentation = lastKey(svalue.pres);

						out += "<tbody id=\"id" + svalue.id + "\">";

						$.each(svalue.pres, function(pkey, pvalue) {

							pvalue.title = highlight(pvalue.title);

							var view = (pvalue.abs != "")
								?["<a href=\"#\" id=\"id" + svalue.id + "-" + pvalue.id + "\" title=\"Preview this presentation\">","</a>","<span></span>"]
								:["","",""];

							var view = ["<a href=\"#\" id=\"id" + svalue.id + "-" + pvalue.id + "\" title=\"Preview this presentation\">","</a>","<span></span>"];
							var active = ($.inArray(pvalue.id, piti) >= 0)?" active":"";
							var hint = ($.inArray(pvalue.id, piti) >= 0)?"Remove presentation from favourites":"Add presentation to favourites";
							var person = (parseInt(pvalue.pers) > 0)?"<span id=\"id" + svalue.id + "-" + pvalue.id + "\" class=\"person\" title=\"This presentation lists a person that you were searching for. Click to preview.\"></span>":"";
							var bottom = (pkey == lastPresentation)?" class=\"bottom\"":"";

							out += "<tr" + bottom + ">"
								+ "<td class=\"color\" style=\"background: #" + svalue.background + ";\"></td>";

							if(svalue.layout == "code-title-time") {
								
								out += "<td class=\"code\">" + view[0] + pvalue.code + view[1] + "</td>"
									+ "<td class=\"title\">" + view[0] + pvalue.title + view[1] + person + "</td>"
									+ "<td class=\"time\">" + pvalue.time + "</td>";
							
							} else if(svalue.layout == "code-title") {
								
								out += "<td class=\"code\">" + view[0] + pvalue.code + view[1] + "</td>"
									+ "<td colspan=\"2\" class=\"title\">" + view[0] + pvalue.title + view[1] + person + "</td>";
							
							} else if(svalue.layout == "title-time") {
								
								out += "<td colspan=\"2\" class=\"title\">" + view[0] + pvalue.title + view[1] + person + "</td>"
									+ "<td class=\"time\">" + pvalue.time + "</td>";
							
							} else if(svalue.layout == "title") {
								
								out += "<td colspan=\"3\" class=\"title\">" + view[0] + pvalue.title + view[1] + person + "</td>";	
							}

							out += "<td class=\"view\">" + view[0] + view[2] + view[1] + "</td>" 
								+ "<td class=\"iti\"><span class=\"id" + svalue.id + "-" + pvalue.id + active + "\" title=\"" + hint + "\"></span></td>"
								+ "</tr>";
						});

						out += "</tbody>";
					}

				});

				if($.inArray(listBlocks[bkey], listBlocksUsed) == -1) {

					out += "</table></div>";

					if(deleteContent) {

						$("div#content").html(out);
						deleteContent = false;

					} else {

						$("div#content").append(out);
					}

					listBlocksUsed.push(listBlocks[bkey]);
				
				} else {

					$("div#" + listBlocks[bkey] + ".block.list table").append(out);
					
					out = "";
				}
			});

		} else {

			out += "<div class=\"empty\">No data found</div>";

			$("div#content").html(out);
		}

		stopLoader();

		setScroll();
		setItinerary();
		setSessions();
		setPresentations();

		if(scroll) $(window).scrollTop(0);
	} 

	function fetchItinerary(scroll) {

		localStorage.esicm2015View = "itinerary";
		startLoader();

		request = $.ajax({

			type: "POST",
			url: "?fetch=itinerary",
			data: { 
				
				search: localStorage.esicm2015Search, 
				filter: {
				
					"days": localStorage.esicm2015FilterDays, 
					"itinerarysessions": localStorage.esicm2015ItinerarySessions,
					"itinerarypresentations": localStorage.esicm2015ItineraryPresentations,
					"groups": localStorage.esicm2015FilterGroups, 
					"types": localStorage.esicm2015FilterTypes, 
					"themes": localStorage.esicm2015FilterThemes, 
					"rooms": localStorage.esicm2015FilterRooms 
				}
			},
			
			dataType: "json",

		}).done(function(data) {

			//console.dir(data);

			itineraryBlocks = data.blocks;
			itineraryData = data.list;
			drawItinerary(scroll);

		}).fail(function(jqXHR, textStatus, errorThrown) {
			
